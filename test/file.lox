class Cake {
  init() {
    print "baking";
  }

  taste() {
    var adjective = "delicious";
    this.flavor = "Faveur";
    print "The " + this.flavor + " cake is " + adjective + "!";
  }
}

class Rect {

  init(size) {
    this.height = size;
    this.width = size;
  }

  area {
    return this.width * this.height;
  }
}

class Math {
  class Pow(a) {
    return a * a;
  }
}

class Parent {
  sayHello() {
    print "hello world";
  }
}

class Child < Parent {
  sayHello() {
    super.sayHello();
    print "hello child";
  }
}

Child().sayHello();

